{
  "name": "ghostbrowser",
  "version": "1.0.0",
  "description": "Open-source antidetect browser â€” Dolphin Anty alternative",
  "main": "dist-electron/main.js",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "assets:icons": "node scripts/generate-icons.mjs",
    "electron:dev": "concurrently \"vite\" \"wait-on http://localhost:5173 && electron .\"",
    "electron:build": "npm run build && electron-builder",
    "preview": "vite preview"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.39.0",
    "electron-store": "^8.1.0",
    "playwright": "^1.41.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.21.0",
    "react-hot-toast": "^2.4.1",
    "lucide-react": "^0.303.0",
    "socks-proxy-agent": "^8.0.2",
    "geoip-lite": "^1.4.10",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.45",
    "@types/react-dom": "^18.2.18",
    "@types/uuid": "^9.0.7",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.16",
    "concurrently": "^8.2.2",
    "electron": "^28.1.0",
    "electron-builder": "^24.9.1",
    "postcss": "^8.4.32",
    "tailwindcss": "^3.4.0",
    "typescript": "^5.3.3",
    "vite": "^5.0.10",
    "vite-plugin-electron": "^0.28.0",
    "wait-on": "^7.2.0"
  },
  "build": {
    "appId": "com.ghostbrowser.app",
    "productName": "GhostBrowser",
    "directories": {
      "output": "release",
      "buildResources": "build"
    },
    "protocols": {
      "name": "GhostBrowser Auth",
      "schemes": ["ghostbrowser"]
    },
    "files": [
      "dist/**/*",
      "dist-electron/**/*"
    ],
    "mac": {
      "target": "dmg",
      "icon": "icon.png"
    },
    "win": {
      "target": "nsis"
    },
    "linux": {
      "target": "AppImage",
      "icon": "icon.png"
    }
  }
}
